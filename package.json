{
  "name": "react-interview-starter-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test",
    "serve": "./node_modules/.bin/http-server ./build --port 1337",
    "e2e:wait-and-run:firefox": "./node_modules/.bin/wait-on http://localhost:1337/ && npm run e2e:run:firefox",
    "e2e:wait-and-run:chrome": "./node_modules/.bin/wait-on http://localhost:1337/ && npm run e2e:run:chrome"
  },
  "browserslist": {
    "characterion": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "@testing-library/jest-dom": "5.11.3",
    "@testing-library/react": "10.4.8",
    "@types/jest": "26.0.10",
    "@types/jquery": "^3.5.2",
    "@types/node": "14.6.0",
    "@types/qs": "^6.9.5",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "5.1.5",
    "@types/uuid": "^8.3.0",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "jquery": "^3.5.1",
    "qs": "^6.9.4",
    "react": "16.13.1",
    "react-async": "^10.0.1",
    "react-dom": "16.13.1",
    "react-modal": "^3.11.2",
    "react-query": "^2.23.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-test-renderer": "16.13.1",
    "react-tooltip": "^4.2.10",
    "typescript": "3.9.7",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/testing-library__jest-dom": "5.9.2",
    "cross-env": "7.0.2",
    "full-icu": "1.3.1",
    "http-server": "0.12.3",
    "wait-on": "5.2.0"
  }
}
